{
  "name": "armor",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "mocha && grunt",
    "global-mac": "sudo npm install mocha nodemon grunt grunt-cli -g",
    "global-win": "npm install mocha nodemon grunt grunt-cli -g",
    "setup-mac": "npm install && npm run createdb && npm run schema-db && npm run add-testdata",
    "setup-win": "cscript WinPostgrInstall.vbs && npm install && createdb armor && createdb testarmor && psql -d armor -f sqlSchems/database.sql && psql -d testarmor -f sqlSchems/database.sql && node data/test/testuser.js",
    "createdb": "createdb armor && createdb testarmor",
    "dropdb": "dropdb armor && dropdb testarmor",
    "schemadb-mac": "psql -d armor -f sqlSchems/database.sql && psql -d testarmor -f sqlSchems/database.sql",
    "backupdb-mac": "cat ./sqlSchems/database.sql > ./sqlSchems/backup/database_`date +\"%d-%m-%Y\"`.sql",
    "dumpdb": "pg_dump armor > ./sqlSchems/database.sql",
    "add-testdata": "node data/test/testglobal.js",
    "test": "mocha"
  },
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "ejs": "^2.4.2",
    "express": "~4.13.1",
    "express-jwt": "^3.4.0",
    "isemail": "^2.2.1",
    "jsonwebtoken": "^7.0.1",
    "lodash": "^4.13.1",
    "morgan": "~1.6.1",
    "nodemailer": "^2.4.2",
    "nodemailer-smtp-transport": "^2.5.0",
    "pg": "^5.1.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "chai": "^3.5.0",
    "grunt": "~1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-concat-css": "*",
    "grunt-concurrent": "^2.3.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-express-server": "^0.5.3",
    "grunt-mocha-test": "^0.12.7",
    "grunt-nodemon": "^0.4.2",
    "mocha": "^2.5.3",
    "supertest": "^1.2.0"
  }
}
